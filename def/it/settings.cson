  subSectionHeadings: [
    {_label: "Invisible", value: "Caratteri invisibili"}
    {_label: "Community Packages", value: "Pacchetti della comunità"}
    {_label: "Core Packages", value: "Pacchetti predefiniti"}
    {_label: "Development Packages", value: "Pacchetti per sviluppare"}
